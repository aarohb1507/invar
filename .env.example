# Required: API key for authenticating ingest requests
# Generate: openssl rand -base64 32
INVAR_API_KEY=your-secret-key-here

# Optional: server port (default 3000)
PORT=3000

# Redis connection (default redis://localhost:6379)
REDIS_URL=redis://localhost:6379

# Redis stream and channel names (defaults shown)
# REDIS_STREAM=invar:ingest
# REDIS_CHANNEL=invar:live
